pdnsmaster: true
need_wsgi_py3: true
apache_vhosts:
  - apt
  - map
letsencrypt_certs:
  - map.ring.nlnog.net

nftables:
  input:
    - name: dns
      proto: udp
      dport: 53
    - name: dns
      proto: tcp
      dport: 53
    - name: http (map)
      proto: tcp
      dport: 80
    - name: https (map)
      proto: tcp
      dport: 443
